.EntranceField{
  display: flex;
  flex-direction: column;
}

.idEnt{
  display: flex;
  width: 100%;
  height: 100rpx;
  border-top-width: 1rpx;
  border-left-width: 0;
  border-right-width: 0;
  border-bottom-width: 1rpx;
  border-style: solid;
  border-color: lightgray;
  background-color: #f2f2f2;
}

.settleEnt,.payEnt,.statusEnt,.serviceEnt{
  display: flex;
  width: 100%;
  height: 80rpx;
  border-top-width: 1rpx;
  border-left-width: 0;
  border-right-width: 0;
  border-bottom-width: 1rpx;
  border-style: solid;
  border-color: lightgray;
  background-color: #f2f2f2;
  margin-top: 30rpx;
}

.idNameTxt{
  font-size: 30rpx;
  padding-left: 10rpx;
  padding-top: 35rpx;
}

.settleTxt,.payTxt,.statusTxt,.serviceTxt{
  font-size: 30rpx;
  padding-left: 10rpx;
  padding-top: 25rpx;
}

.payNum,.statusNum{
  font-size: 30rpx;
  padding-left: 580rpx;
  padding-top: 25rpx;
}



